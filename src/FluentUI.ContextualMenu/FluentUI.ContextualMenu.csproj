<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>FluentUI.ContextualMenu</AssemblyName>
    <RootNamespace>FluentUI</RootNamespace>
    <Nullable>disable</Nullable>
    <RepositoryUrl>https://github.com/vnbaaij/FluentUI</RepositoryUrl>
    <PackageProjectUrl>https://github.com/vnbaaij/FluentUI</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\contextualMenu.ts" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="wwwroot\contextualMenu.ts" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
    <PackageReference Include="Microsoft.TypeScript.MSBuild" Version="4.0.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FluentUI.BaseComponent\FluentUI.BaseComponent.csproj" />
    <ProjectReference Include="..\FluentUI.Callout\FluentUI.Callout.csproj" />
    <ProjectReference Include="..\FluentUI.FocusZone\FluentUI.FocusZone.csproj" />
    <ProjectReference Include="..\FluentUI.Icon\FluentUI.Icon.csproj" />
    <ProjectReference Include="..\FluentUI.ResponsiveLayout\FluentUI.ResponsiveLayout.csproj" />
  </ItemGroup>

</Project>
