<?xml version="1.0" encoding="utf-8"?>

<Project>

	<PropertyGroup>
		<Authors>Vincent Baaij, Lee McPherson, Lukas Frensel and others</Authors>
		<Owners>Vincent Baaij, Lee McPherson</Owners>
		<Company></Company>
		<RepositoryUrl>https://github.com/FluentUI/FluentUI</RepositoryUrl>
		<PackageProjectUrl>https://github.com/FluentUI/FluentUI</PackageProjectUrl>
		<Product>FluentUI</Product>
		<Copyright>Copyright 2020</Copyright>
		<PackageTags>Blazor;FluentUI;</PackageTags>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<IsPackable>true</IsPackable>
		<CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
		<PackageOutputPath>../../packages</PackageOutputPath>
		<Version>5.0.0</Version>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>

	</PropertyGroup>

	<ItemGroup Condition="$(ProjectName.Contains('Test')) == false">
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<!--<ItemGroup Condition="$(ProjectName.Contains('BaseComponent')) == false" >
		<Content Include="..\FluentUI.BaseComponent\tsconfig.json">

		</Content>
	</ItemGroup>-->

</Project>
