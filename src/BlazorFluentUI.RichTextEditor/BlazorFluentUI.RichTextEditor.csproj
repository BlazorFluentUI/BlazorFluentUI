<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>BlazorFluentUI.RichTextEditor</AssemblyName>
    <Description>RichTextEditor component from Blazor port of Fluent UI React library. Using quilljs</Description>
    <RootNamespace>BlazorFluentUI</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.3" />
    <PackageReference Include="Microsoft.TypeScript.MSBuild" Version="4.2.2">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\BlazorFluentUI.BaseComponent\BlazorFluentUI.BaseComponent.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.CommandBar\BlazorFluentUI.CommandBar.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.Dialog\BlazorFluentUI.Dialog.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.Stack\BlazorFluentUI.Stack.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.TextField\BlazorFluentUI.TextField.csproj" />
  </ItemGroup>

</Project>
