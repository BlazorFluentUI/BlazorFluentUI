    <ProjectReference Include="..\FluentUI.Checkbox\FluentUI.Checkbox.csproj" />
    <ProjectReference Include="..\FluentUI.FocusZone\FluentUI.FocusZone.csproj" />
    <ProjectReference Include="..\FluentUI.Icon\FluentUI.Icon.csproj" />
    <ProjectReference Include="..\FluentUI.Keytip\FluentUI.Keytip.csproj" />
    <ProjectReference Include="..\FluentUI.Label\FluentUI.Label.csproj" />
    <ProjectReference Include="..\FluentUI.Panel\FluentUI.Panel.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
  </ItemGroup>

</Project>
