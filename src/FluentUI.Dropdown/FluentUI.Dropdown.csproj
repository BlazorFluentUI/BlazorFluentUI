<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>FluentUI.Dropdown</AssemblyName>
    <RootNamespace>FluentUI</RootNamespace>
    <Nullable>disable</Nullable>
    <RepositoryUrl>https://github.com/vnbaaij/FluentUI</RepositoryUrl>
    <PackageProjectUrl>https://github.com/vnbaaij/FluentUI</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
