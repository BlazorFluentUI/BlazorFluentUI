<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>BlazorFluentUI.Panel</AssemblyName>
		<Description>Panel component from Blazor port of Fluent UI React library</Description>
		<RootNamespace>BlazorFluentUI</RootNamespace>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<Content Remove="tsconfig.json" />
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.3" />
		<PackageReference Include="Microsoft.TypeScript.MSBuild" Version="4.2.2">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>


	<ItemGroup>
		<ProjectReference Include="..\BlazorFluentUI.BaseComponent\BlazorFluentUI.BaseComponent.csproj" />
		<ProjectReference Include="..\BlazorFluentUI.Button\BlazorFluentUI.Button.csproj" />
		<ProjectReference Include="..\BlazorFluentUI.FocusTrapZone\BlazorFluentUI.FocusTrapZone.csproj" />
		<ProjectReference Include="..\BlazorFluentUI.Overlay\BlazorFluentUI.Overlay.csproj" />
	</ItemGroup>


	<ItemGroup>
		<TypeScriptCompile Include="tsconfig.json">
			<CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</TypeScriptCompile>
	</ItemGroup>
</Project>
