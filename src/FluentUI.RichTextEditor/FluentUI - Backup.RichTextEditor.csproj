<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>BlazorFluentUI.RichTextEditor</AssemblyName>
    <RootNamespace>BlazorFluentUI</RootNamespace>
    <Nullable>disable</Nullable>
    <RepositoryUrl>https://github.com/BlazorFluentUI/BlazorFluentUI</RepositoryUrl>
    <PackageProjectUrl>https://github.com/BlazorFluentUI/BlazorFluentUI</PackageProjectUrl>
    <Description>RichTextEditor built using Fabric components and quilljs</Description>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
    <PackageReference Include="Microsoft.TypeScript.MSBuild" Version="4.0.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\BlazorFluentUI.BaseComponent\BlazorFluentUI.BaseComponent.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.CommandBar\BlazorFluentUI.CommandBar.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.Dialog\BlazorFluentUI.Dialog.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.Stack\BlazorFluentUI.Stack.csproj" />
    <ProjectReference Include="..\BlazorFluentUI.TextField\BlazorFluentUI.TextField.csproj" />
  </ItemGroup>

</Project>
